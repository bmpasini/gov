<h1>Listing legislations</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Proposal date</th>
      <th>Publication date</th>
      <th>Congress voting date</th>
      <th>Senate voting date</th>
      <th>President signing date</th>
      <th>Abstract</th>
      <th>Url</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @legislations.each do |legislation| %>
      <tr>
        <td><%= legislation.title %></td>
        <td><%= legislation.proposal_date %></td>
        <td><%= legislation.publication_date %></td>
        <td><%= legislation.congress_voting_date %></td>
        <td><%= legislation.senate_voting_date %></td>
        <td><%= legislation.president_signing_date %></td>
        <td><%= legislation.abstract %></td>
        <td><%= legislation.url %></td>
        <td><%= legislation.type %></td>
        <td><%= link_to 'Show', legislation %></td>
        <td><%= link_to 'Edit', edit_legislation_path(legislation) %></td>
        <td><%= link_to 'Destroy', legislation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Legislation', new_legislation_path %>
