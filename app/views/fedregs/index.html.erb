<div id="frame">
    <ul class="slidee">
      <% @result_set.each do |data| %>
        <li>
          <h1><%= data.title %></h1>
          <% data.agencies.each do |agency| %>
            <p><a href="<%= agency.url %>">
              <%= agency.name %>
            </a></p>
          <% end %>
          <p><%= data.abstract %></p>
          <p><%= data.type %></p>
          <p><%= data.publication_date %></p>
        </li>
      <% end %>
    </ul>
  </div>
</div>
